<div class="content-block video-block">
	<% if block['source'] == 'vimeo' %>
	  <iframe src="<%= protocol %>player.vimeo.com/video/<%= block['remote_id'] %>?title=1&amp;byline=1&amp;portrait=1&amp;autoplay=0" width="640" height="360" frameborder="0" webkitAllowFullScreen mozallowfullscreen allowFullScreen></iframe>
	<% elsif block['source'] == 'youtube' %>
	  <iframe width="640" height="360" src="<%= protocol %>www.youtube.com/embed/<%= block['remote_id'] %>" frameborder="0" allowfullscreen></iframe>
	<% end %>
</div>