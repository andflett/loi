.logo_grid {
	padding: 0;
	margin: 0;
	list-style-type: none;
}

.logo_grid li {
	display: inline-block;
	width: 231px;
	height: 130px;
	margin: 0 20px 20px 0;
	padding: 0;
}

.logo_grid li:nth-child(4n+4) {
	margin-right: 0;
}

.logo_grid li a,
.logo_grid li a img {
	display: block;
}

.logo_grid li:hover {
	border-style: solid;
}