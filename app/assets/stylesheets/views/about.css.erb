.view_about #primary_content #schemantics {
	float: left;
	background: #cee9e7;
	width: 600px;
}

.view_about #primary_content #schemantics h1 {
  width: 610px;
  left: -5px;
}

.view_about #primary_content aside {
	width: 300px;
	float: right;
	padding: 20px;
	background: #cee9e7;
	border-radius: 5px;	
}

.view_about #primary_content aside h2 {
	color: #E40F4F;
	margin-bottom: 25px;
  border-bottom: 1px solid #E40F4F;
  font-family: bentonsansmedium;
  padding-bottom: 5px;
}

.logo_grid {
	padding: 0;
	margin: 0;
	list-style-type: none;
}

.logo_grid li {
	display: block;
	float: left;
	width: 92px;
	height: 52px;
	margin: 0 10px 10px 0;
	padding: 0;
	overflow: hidden;
	border-radius: 5px;
}

.logo_grid li:nth-child(3n+3) {
	margin-right: 0;
}

.logo_grid li a,
.logo_grid li a img {
	display: block;
}


.view_join #primary_content #schemantics {
	float: left;
	width: 630px;
}

.view_join #primary_content aside {
	width: 300px;
	float: right;
}